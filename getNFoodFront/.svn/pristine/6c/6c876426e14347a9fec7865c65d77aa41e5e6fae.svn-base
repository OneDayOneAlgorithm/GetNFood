.title {
    color: black;
    font-weight: bold;
    margin-top: 10vh;
    margin-left: 5vw;
    font-size: 1.5em;
    margin-bottom: 10vh;
}

.container {
    display: grid;
    grid-template-columns: 1fr 1fr 1fr;
    grid-template-rows: 40vh 40vh;
    margin-left: 5vw;
    margin-right: 5vw;
    row-gap: 2vh;
}



.container * {
    border: 1px solid #DBDBDB;
    border-radius: 10px;
}

.first {
    margin-right: 2vw;

}

.fourth {
    margin-right: 2vw;
}

.second {
    margin-right: 2vw;

}

.fifth {
    margin-right: 2vw;
}